{
    "version": "0.2.0",
    "inputs": [
        {
            "id": "package",
            "type": "promptString",
            "description": "Package name",
            "default": "china_lidar_driver"
        },
        {
            "id": "program",
            "type": "promptString",
            "description": "Program name",
            "default": "data_reader"
        }
    ],
    "configurations": [
      {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/${input:package}/${input:program}",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "preLaunchTask": "Build [Debug]",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
      },
    ]
}